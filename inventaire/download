    <script>
        // preselect the tab of the OS used by the visitor
        const localOs =
            (navigator.appVersion.indexOf('Win') !== -1) ? 'windows'
            : (navigator.appVersion.indexOf('Mac') !== -1) ? 'macos'
            : 'linux';

        document.getElementById('tab-client-' + localOs).classList.add('active');
        document.getElementById('client-' + localOs).classList.add('in', 'active');
    </script>

